      "webp": true,
      "lossy": false,
      "lossless": true
    },
    "mirage": {
      "enabled": true,
      "description": "Accelerates image delivery on mobile devices"
    },
    "rocket_loader": {
      "enabled": false,
      "description": "Disabled for better compatibility with IT-ERA scripts"
    }
  },
  "page_rules_performance": [
    {
      "url_pattern": "it-era.it/*.css",
      "settings": {
