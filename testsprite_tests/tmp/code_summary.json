        "openapi": "3.0.0",
        "info": {
          "title": "Swarm Orchestration System",
          "version": "1.0.0",
          "description": "Multi-agent AI coordination with consensus building and A/B testing"
        },
        "components": {
          "schemas": {
            "SwarmResponse": {
              "type": "object",
              "properties": {
                "response": {"type": "string"},
                "agentsUsed": {"type": "array", "items": {"type": "string"}},
                "consensusStrength": {"type": "number"},
                "leadScore": {"type": "number"},
                "cost": {"type": "number"}
              }
            }
          }
        }
      }
