{
  "tech_stack": [
    "JavaScript",
    "Node.js", 
    "Cloudflare Workers",
    "Cloudflare KV Storage",
    "OpenRouter API",
    "Microsoft Teams Webhooks",
    "AI/ML (Claude, GPT-4, DeepSeek)",
    "REST APIs",
    "Wrangler CLI",
    "Jest"
  ],
  "features": [
    {
      "name": "AI-Enhanced Chatbot API",
      "description": "Main chatbot endpoint with AI integration, swarm orchestration, and business rule protection",
      "files": [
        "api/src/chatbot/api/chatbot-worker-simple.js",
        "api/wrangler-chatbot.toml"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {
          "title": "IT-ERA Chatbot API",
          "version": "2.0.0",
          "description": "AI-powered chatbot with swarm intelligence and business protection"
        },
        "servers": [
          {
            "url": "https://it-era-chatbot-staging.bulltech.workers.dev",
            "description": "Staging environment"
          }
        ],
        "paths": {
          "/api/chat": {
            "post": {
              "summary": "Process chat messages",
              "description": "Main endpoint for chatbot conversations with AI, swarm orchestration, and lead scoring",
              "requestBody": {
                "required": true,
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "message": {"type": "string", "description": "User message"},
                        "sessionId": {"type": "string", "description": "Chat session identifier"},
                        "action": {"type": "string", "enum": ["start", "message"], "description": "Action type"}
                      },
                      "required": ["action"]
                    }
                  }
                }
              },
              "responses": {
                "200": {
                  "description": "Successful chat response",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "success": {"type": "boolean"},
                          "sessionId": {"type": "string"},
                          "response": {"type": "string"},
                          "options": {"type": "array", "items": {"type": "string"}},
                          "intent": {"type": "string"},
                          "confidence": {"type": "number"},
                          "leadScore": {"type": "number"},
                          "source": {"type": "string", "enum": ["ai", "swarm", "fallback"]},
                          "cost": {"type": "number"}
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "/health": {
            "get": {
              "summary": "Health check endpoint",
              "description": "Returns system status, AI availability, and performance metrics",
              "responses": {
                "200": {
                  "description": "Health status",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "status": {"type": "string"},
                          "service": {"type": "string"},
                          "ai": {
                            "type": "object",
                            "properties": {
                              "enabled": {"type": "boolean"},
                              "initialized": {"type": "boolean"},
                              "provider": {"type": "string"},
                              "model": {"type": "string"}
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Swarm Intelligence Orchestration",
      "description": "Multi-agent AI system with 8 specialized agents for lead qualification, technical advice, and business protection",
      "files": [
        "api/src/chatbot/swarm/chatbot-swarm-integration.js",
