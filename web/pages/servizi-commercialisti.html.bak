    <!-- Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    
    <!-- Custom JavaScript -->
    <script>
        // FAQ Toggle
        document.querySelectorAll('.accounting-faq-question').forEach(button => {
            button.addEventListener('click', () => {
                const answer = button.nextElementSibling;
                const icon = button.querySelector('i');
                
                answer.classList.toggle('active');
                icon.classList.toggle('fa-chevron-down');
                icon.classList.toggle('fa-chevron-up');
            });
        });

        // Accounting Form Submission
        document.getElementById('accountingLeadForm').addEventListener('submit', async (e) => {
            e.preventDefault();
            
            const formData = new FormData(e.target);
            const data = Object.fromEntries(formData.entries());
            
            // Add accounting-specific data
            data.sector_page = 'Commercialisti';
            data.source = 'Accounting Sector Page';
            data.timestamp = new Date().toISOString();
            data.service_priority = 'Cloud Migration';
            
            try {
                const response = await fetch('/api/leads', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify(data)
                });
                
                if (response.ok) {
                    alert('Grazie! Ti ricontatteremo entro 2 ore per la tua migrazione cloud gratuita.');
                    e.target.reset();
                    
                    // Track accounting conversion
                    if (typeof gtag !== 'undefined') {
                        gtag('event', 'conversion', {
                            'send_to': 'AW-CONVERSION_ID/ACCOUNTING_CONVERSION',
                            'event_category': 'accounting_lead',
                            'event_label': 'commercialisti-page'
                        });
                    }
                } else {
                    throw new Error('Errore nell\'invio');
                }
            } catch (error) {
                alert('Errore nell\'invio. Chiama direttamente il 039 888 2041');
                console.error('Accounting form submission error:', error);
            }
        });

        // Smooth scrolling
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });

        // Cloud Migration Badge Animation
        const cloudBadge = document.querySelector('.cloud-migration-badge');
        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.style.animation = 'pulse 3s infinite';
                }
            });
        });
        
        if (cloudBadge) {
            observer.observe(cloudBadge);
        }

        // Stats Counter Animation
        const statsCards = document.querySelectorAll('.accounting-stat-card');
        const statsObserver = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.style.animation = 'fadeInUp 0.6s ease-out';
                }
            });
        }, { threshold: 0.3 });
        
        statsCards.forEach(card => {
            statsObserver.observe(card);
        });
    </script>

    <!-- Additional CSS for animations -->
    <style>
        @keyframes fadeInUp {
            from {
                opacity: 0;
                transform: translateY(30px);
            }
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }
    </style>
