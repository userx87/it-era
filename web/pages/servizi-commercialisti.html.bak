            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 30px;
            max-width: 1000px;
            margin: 50px auto;
        }
        
        .process-step {
            background: rgba(255,255,255,0.1);
            padding: 30px 25px;
            border-radius: 15px;
            text-align: center;
            position: relative;
            backdrop-filter: blur(10px);
            border: 1px solid rgba(255,255,255,0.2);
        }
        
        .step-number {
