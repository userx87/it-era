    <!-- Critical CSS inline -->
    <style>
        :root {
            --accounting-primary: #1a5490;
            --accounting-secondary: #0ea5e9;
            --accounting-accent: #06b6d4;
            --accounting-success: #10b981;
            --accounting-warning: #f59e0b;
            --accounting-danger: #ef4444;
            --accounting-light: #f1f5f9;
            --accounting-dark: #1e293b;
        }
        
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        
        body {
            font-family: 'Inter', -apple-system, BlinkMacSystemFont, sans-serif;
            line-height: 1.6;
            color: #333;
            padding-top: 80px;
        }
        
        .hero-accounting {
            background: linear-gradient(135deg, var(--accounting-primary) 0%, var(--accounting-secondary) 100%);
            color: white;
            padding: 100px 0;
            position: relative;
            overflow: hidden;
        }
        
        .hero-accounting::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: url('data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 20" fill="white" opacity="0.08"><circle cx="20" cy="10" r="1"/><circle cx="40" cy="10" r="1"/><circle cx="60" cy="10" r="1"/><circle cx="80" cy="10" r="1"/></svg>') repeat;
        }
        
        .hero-content {
            position: relative;
            z-index: 2;
        }
        
        .cloud-migration-badge {
            display: inline-flex;
            align-items: center;
            gap: 10px;
            background: rgba(255,255,255,0.2);
            padding: 12px 24px;
            border-radius: 30px;
            margin-bottom: 30px;
            border: 1px solid rgba(255,255,255,0.3);
            animation: pulse 3s infinite;
        }
        
        @keyframes pulse {
            0%, 100% { transform: scale(1); }
            50% { transform: scale(1.05); }
        }
        
        .hero-accounting h1 {
            font-size: clamp(2.5rem, 5vw, 4rem);
            font-weight: 700;
            margin-bottom: 20px;
            text-shadow: 0 2px 4px rgba(0,0,0,0.3);
        }
        
        .hero-accounting .lead {
            font-size: clamp(1.2rem, 3vw, 1.6rem);
            margin-bottom: 40px;
            opacity: 0.95;
            max-width: 700px;
        }
        
        .accounting-stats {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(180px, 1fr));
            gap: 25px;
            margin-top: 50px;
        }
        
        .accounting-stat-card {
            background: rgba(255,255,255,0.15);
            padding: 25px 20px;
            border-radius: 15px;
            text-align: center;
            backdrop-filter: blur(10px);
            border: 1px solid rgba(255,255,255,0.2);
            transition: transform 0.3s ease;
        }
        
        .accounting-stat-card:hover {
            transform: translateY(-5px);
        }
        
        .stat-number {
            font-size: 2.5rem;
            font-weight: 700;
            display: block;
            margin-bottom: 5px;
        }
        
        .stat-label {
            font-size: 0.9rem;
            opacity: 0.9;
        }
        
        .btn-accounting-primary {
            display: inline-flex;
            align-items: center;
            gap: 10px;
            background: white;
            color: var(--accounting-primary);
            padding: 18px 36px;
            font-size: 1.2rem;
            font-weight: 600;
            border: none;
            border-radius: 50px;
            text-decoration: none;
            transition: all 0.3s ease;
            box-shadow: 0 4px 20px rgba(0,0,0,0.2);
            margin-right: 20px;
        }
        
        .btn-accounting-primary:hover {
            transform: translateY(-2px);
            box-shadow: 0 6px 25px rgba(0,0,0,0.3);
            color: var(--accounting-primary);
        }
        
        .btn-accounting-secondary {
            display: inline-flex;
            align-items: center;
            gap: 10px;
            background: transparent;
            color: white;
            padding: 18px 36px;
            font-size: 1.1rem;
            font-weight: 500;
            border: 2px solid white;
            border-radius: 50px;
            text-decoration: none;
            transition: all 0.3s ease;
        }
        
        .btn-accounting-secondary:hover {
            background: white;
            color: var(--accounting-primary);
        }
        
        .cloud-benefits-section {
            padding: 80px 20px;
            background: var(--accounting-light);
        }
        
        .benefits-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 30px;
            max-width: 1200px;
            margin: 50px auto;
        }
        
        .benefit-card {
            background: white;
            padding: 40px 30px;
            border-radius: 15px;
            box-shadow: 0 5px 20px rgba(0,0,0,0.08);
            transition: all 0.3s ease;
            border-left: 4px solid var(--accounting-primary);
            position: relative;
            overflow: hidden;
        }
        
        .benefit-card::before {
            content: '';
            position: absolute;
            top: 0;
            right: 0;
            width: 60px;
            height: 60px;
            background: linear-gradient(135deg, var(--accounting-primary), var(--accounting-secondary));
            opacity: 0.1;
            border-radius: 0 15px 0 50px;
        }
        
        .benefit-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 10px 30px rgba(0,0,0,0.12);
        }
        
        .benefit-icon {
            width: 80px;
            height: 80px;
            background: linear-gradient(135deg, var(--accounting-primary), var(--accounting-secondary));
            border-radius: 20px;
